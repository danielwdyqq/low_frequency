\glossaryentry{v1.3> =General:>Added `lofdepth' and `lotdepth'. }{7}
\glossaryentry{v1.3> =General:>Added `caption'.}{7}
\glossaryentry{v1.2> =General:>Added missing space.}{7}
\glossaryentry{v1.2> =General:>Fixed references.}{10}
